{
  "name": "axelduret/hadron",
  "authors": [
    {
      "name": "Axel Duret"
    }
  ],
  "title": "Hadron Project",
  "description": "Hadron Domain Driven Design (ParticleBC + HadronBC) demo with CQRS, Event Sourcing, gRPC stubs, REST, MkDocs",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "ramsey/uuid": "^4.7",
    "slim/slim": "^4.10",
    "nyholm/psr7": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mkdocs/mkdocs": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}